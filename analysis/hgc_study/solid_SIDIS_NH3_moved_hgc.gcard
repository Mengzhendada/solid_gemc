<gcard>

<detector name="../../geometry/hgc_moved/solid_SIDIS_hgc" factory="TEXT" variation="Original"/>
<detector name="solid_SIDIS_target_NH3" factory="TEXT" variation="Original"/>

<option name="INTEGRATEDRAW" value="flux"/>
<option name="RECORD_PASSBY" value="1"/>
<option name="SAVE_ALL_MOTHERS" value="1"/>

<option name="HIT_PROCESS_LIST" value="solid"/>
<option name="PHYSICS" value="QGSP_BERT+STD+Optical"/>
<option name="HALL_MATERIAL" value="G4_Galactic"/>

<option name="FIELD_DIR" value="../../field"/>
<option name="HALL_FIELD" value="solenoid_CLEOv9"/>

<option name="MAX_X_POS" value="20000"/>
<option name="MAX_Y_POS" value="20000"/>
<option name="MAX_Z_POS" value="20000"/>
<option name="HALL_DIMENSIONS" value="20*m,20*m,20*m"/>

<option name="BEAM_P"   value="pi-,5*GeV,8*deg,0*deg"/>
<option name="SPREAD_P" value="3*GeV,0*deg,180*deg"/>
<option name="BEAM_V"   value="(0,0,-350)cm"/>
<option name="SPREAD_V" value="(0.0,0)cm"/>

<option name="EXEC_MACRO" value="solid_slice.vis"/>
<option name="geometry" value="1000x1000"/>
<option name="HIGH_RES" value="1"/>
<option name="USE_GUI"  value="1"/>


</gcard>

